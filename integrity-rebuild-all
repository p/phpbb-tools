#!/bin/sh

set -e

genp() {
	for branch in develop olympus; do
		for db in mysql mysqli postgres; do
			for target in unit functional wolis; do
				echo "$target-$branch-$db"
			done
		done
	done
}

for project in \
	`genp` \
	unit-develop-nodb \
	unit-olympus-nodb \
; do
	curl -nX POST http://integrity.vps.hxr.me/$project/builds
done
